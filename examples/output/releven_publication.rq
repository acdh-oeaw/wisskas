PREFIX crm: <http://www.cidoc-crm.org/cidoc-crm/>
PREFIX lrmoo: <http://iflastandards.info/ns/lrm/lrmoo/>
PREFIX star: <https://r11.eu/ns/star/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX r11: <https://r11.eu/ns/spec/>
PREFIX r11pros: <https://r11.eu/ns/prosopography/>

SELECT ?publication ?publication__publication_reference ?publication__publication_creation ?publication__publication_creation__publication_creation_event
WHERE {
?publication a r11:Publication.

    ?publication crm:P3_has_note ?publication__publication_reference.

    OPTIONAL { 
    ?publication__publication_creation a star:E13_lrmoo_R24.
    ?publication__publication_creation crm:P141_assigned ?publication.
    
        ?publication__publication_creation__publication_creation_event a lrmoo:F30_Manifestation_Creation.
        ?publication__publication_creation___pce crm:P141_assigned ?publication__publication_creation.
        ?publication__publication_creation___pce crm:P140_assigned_attribute_to ?publication__publication_creation__publication_creation_event.
    }
}

